{
  "name": "mufflermanos",
  "version": "1.0.0",
  "description": "VR-synced control system for PGE mechanics node at 9305 S Orange Blossom Trail",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node --esm src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rimraf dist",
    "init": "node --loader ts-node/esm scripts/init-mufflermanos.ts",
    "test": "jest --passWithNoTests",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "vr",
    "diagnostics",
    "mechanics",
    "pge",
    "genesis-engine",
    "automotive",
    "real-time",
    "websockets"
  ],
  "author": "MKWorldWide",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^11.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "node-cron": "^3.0.3",
    "multer": "^1.4.5-lts.1",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/fs-extra": "^11.0.4",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "@types/multer": "^1.4.11",
    "@types/compression": "^1.7.5",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 