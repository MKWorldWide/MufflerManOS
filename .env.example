# MufflerManOS Environment Configuration
# Copy this file to .env and update with your values

# Server Configuration
PORT=3000
HOST=localhost
NODE_ENV=development

# Database Configuration
DB_TYPE=sqlite
DB_PATH=./data/mufflermanos.db

# Genesis Engine Configuration
PGE_SYNC_ENABLED=true
PGE_SYNC_INTERVAL=30000

# VR Configuration
VR_ENABLED=true
VR_UPDATE_INTERVAL=1000

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/mufflermanos.log

# Security
CORS_ORIGIN=http://localhost:3000
HELMET_ENABLED=true

# API Keys (add your actual keys here)
# OBD2_API_KEY=your_obd2_api_key_here
# DIAGNOSTICS_API_KEY=your_diagnostics_api_key_here 